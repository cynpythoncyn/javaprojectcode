import java.awt.*;
import javax.swing.*;

public class BallGame extends JFrame {
	
	
	
	void loadwindows() {
		setSize(300,300);
		setLocation(40,40);
		setVisible(true);
		
	}

	// 这是java主函数入口
	public static void main(String[] args) {

		System.out.println("桌球游戏");
		
		BallGame game = new BallGame();
		game.loadwindows();

	}

}
